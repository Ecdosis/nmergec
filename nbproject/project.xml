<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.cnd.makeproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/make-project/1">
            <name>NMergeC</name>
            <c-extensions>c</c-extensions>
            <cpp-extensions/>
            <header-extensions>h</header-extensions>
            <sourceEncoding>UTF-8</sourceEncoding>
            <make-dep-projects>
                <make-dep-project>plugins/mvd_export</make-dep-project>
                <make-dep-project>mvd</make-dep-project>
                <make-dep-project>plugins/mvd_list</make-dep-project>
                <make-dep-project>plugins/mvd_add</make-dep-project>
            </make-dep-projects>
            <sourceRootList>
                <sourceRootElem>mvd/src</sourceRootElem>
                <sourceRootElem>plugins/mvd_list</sourceRootElem>
                <sourceRootElem>include/zip</sourceRootElem>
            </sourceRootList>
            <confList>
                <confElem>
                    <name>Debug</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>Release</name>
                    <type>1</type>
                </confElem>
            </confList>
        </data>
    </configuration>
</project>
